{
  "name": "git-configure",
  "displayName": "git-configure",
  "description": "In vscode, config GPG key to git config ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "SCM Providers"
  ],
  "activationEvents": [
    "onCommand:gpg-configure.config",
    "onCommand:git-config.userNameAndEmail"
  ],
  "repository": {},
  "publisher": "windsonR",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gpg-configure.config",
        "title": "%command.gpg.config.title%"
      },
      {
        "command": "git-config.userNameAndEmail",
        "title": "%command.git.config.usernameAndEmail.title%"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "build": "pnpm vsce package --no-dependencies",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/l10n-dev": "^0.0.18",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@vscode/l10n": "^0.0.10"
  },
  "l10n": "./l10n"
}
